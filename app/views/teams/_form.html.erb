<%= form_for @team do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :leader_name %><br />
    <%= f.text_field :leader_name %>
  </p>
  <p>
    <%= f.label :leader_email %><br />
    <%= f.text_field :leader_email %>
  </p>
  <p>
    <%= f.label :departure %><br />
    <%= f.date_select :departure %>
  </p>
  <p>
    <%= f.label :return %><br />
    <%= f.date_select :return %>
  </p>
  <p>
    <%= f.label :destination %><br />
    <%= f.text_field :destination %>
  </p>
  <p>
    <%= f.label :trip_cost %><br />
    <%= f.text_field :trip_cost %>
  </p>
  <p><%= f.submit %></p>
<% end %>
